<?xml version="1.0" encoding="utf-8" ?>
<TabbedPage Title="{Binding Name}"
            xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="PracticeXamarin.CourseMasterPage">
    <TabbedPage.ToolbarItems>
        <ToolbarItem x:Name="EditCourseButton" Text="Edit Term" Activated="EditCourseButton_Activated" Order="Secondary" />
        <ToolbarItem x:Name="DeleteCourseButton" Text="Delete Term" Activated="DeleteCourseButton_Activated" Order="Secondary" />
    </TabbedPage.ToolbarItems>
</TabbedPage>